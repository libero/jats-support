<article xmlns:mml="http://www.w3.org/1998/Math/MathML" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:ali="http://www.niso.org/schemas/ali/1.0/" article-type="research-article">
    <front>
        <article-meta>
            <!-- 
                * 1 of the following is required in article-meta:
                    - article-id[@pub-id-type="publisher-id"]
                    - article-id[@pub-id-type="doi"]
                    - article-id[not(@pub-id-type='pmid') and not(@pub-id-type='pmc') and not(@pub-id-type='doi')] 
                    - elocation-id 
                 Between 1 and all are allowed.
                 
                 * 1 title-group is required in article-meta
            -->
            <article-id pub-id-type="publisher-id">00001</article-id>
            <article-id pub-id-type="doi">10.0000/00001</article-id>
            <article-categories>
                <!-- subj-group-type="heading" treated as article type -->
                <subj-group subj-group-type="heading">
                    <subject>Research Article</subject>
                </subj-group>
                <!-- 
                    Any subj-group[not(subj-group-type="heading")] treated as a research category or subject.
                    Only the first subject is used (subsequent or nested subjects are currently ignored).
                    This XPath (2.0+ needed) will show all the research categories from the below:
                    //article-categories/subj-group[not(@subj-group-type="heading")]/subject[1]/string()
                -->
                <subj-group subj-group-type="subjects">
                    <subject><italic>Cancer Biology</italic></subject>
                </subj-group>
                <subj-group>
                    <subject>Data</subject>
                </subj-group>
                <subj-group subj-group-type="level-1">
                    <subject>Ecology</subject>
                </subj-group>
                <subj-group subj-group-type="any-value">
                    <subject><inline-formula><mml:math id="i1" display="inline"><mml:mover accent="true"><mml:mi>α</mml:mi><mml:mo>^</mml:mo></mml:mover></mml:math></inline-formula></subject>
                </subj-group>
            </article-categories>
            <title-group><bold>Libero</bold> <italic>Kitchen</italic> Sink<sup>2</sup><sub>2</sub></title-group>
            <contrib-group>
